{
  "name": "europa-ui",
  "version": "0.3.25",
  "main": "main.js",
  "private": true,
  "description": "An UI used for deploying and testing contracts on the Europa node embedded",
  "dependencies": {
    "@polkadot/api": "^4.15.2-2",
    "@polkadot/api-contract": "^4.15.2-2",
    "@polkadot/react-identicon": "^0.81.2-0",
    "@polkadot/types": "^4.15.2-2",
    "@polkadot/ui-keyring": "^0.81.2-0",
    "@polkadot/ui-settings": "^0.81.2-0",
    "antd": "^4.15.4",
    "i18next": "^20.3.0",
    "i18next-browser-languagedetector": "^6.1.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.2",
    "react-i18next": "^11.9.0",
    "react-router-dom": "^5.1.7",
    "react-tooltip": "^4.2.20",
    "styled-components": "^5.3.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.170",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.5",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/store": "^2.0.2",
    "@types/styled-components": "^5.1.9",
    "cross-env": "7.0.3",
    "customize-cra": "^1.0.0",
    "electron": "^12.0.7",
    "electron-builder": "^22.11.1",
    "react-app-rewire-alias": "^1.0.3",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "4.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "update:polkadot": "yarn add @polkadot/api@latest @polkadot/api-contract@latest @polkadot/ui-keyring@latest @polkadot/ui-settings@latest && yarna add @polkadot/typegen -D",
    "generate:defs": "tsc --paths type --outDir types &&  node_modules/.bin/polkadot-types-from-defs.cmd --package europa-ui/interfaces --input ./types/interfaces",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "start:electron": "cross-env ElECTRON_ENV=development electron ./",
    "pack:win:dir": " electron-builder --dir",
    "pack:win": "electron-builder --win",
    "pack:mac:dir": "electron-builder --mac --dir",
    "pack:mac": "electron-builder --mac",
    "pack:linux:dir": "electron-builder --linux --dir",
    "pack:linux": "electron-builder --linux",
    "rename": "node ./.github/workflows/rename",
    "rename:artifact": "node ./.github/workflows/rename-artifact"
  },
  "build": {
    "asarUnpack": [
      "resources/europa.exe",
      "resources/europa"
    ],
    "appId": "com.patractlabs.europa-ui",
    "productName": "europa-ui",
    "copyright": "Â© 2021 Patract Labs Co., Limited, All Rights Reserved.",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "icon": "resources/256x256.icns"
    },
    "linux": {
      "target": "AppImage",
      "category": "Development",
      "icon": "resources",
      "desktop": "resources/desktop.desktop"
    },
    "win": {
      "target": "nsis",
      "icon": "resources/256x256.ico"
    },
    "directories": {
      "buildResources": "resources"
    },
    "files": [
      "build/**/*",
      "resources",
      "main.js"
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "installerLanguages": [
        "zh_CN"
      ],
      "language": "0804",
      "runAfterFinish": true,
      "createDesktopShortcut": true,
      "shortcutName": "europa-ui",
      "allowElevation": true,
      "unicode": true
    },
    "extends": null
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
